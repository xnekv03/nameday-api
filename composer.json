{
  "name": "xnekv03/nameday-api",
  "description": "Nameday API library for https://nameday.abalin.net",
  "type": "library",
  "keywords": [
    "api",
    "nameday",
    "name day",
    "international"
  ],
  "homepage": "https://github.com/xnekv03/api_nameday",
  "license": "MIT",
  "authors": [
    {
      "name": "VojtÄ›ch Nekvapil",
      "email": "fby@post.cz",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "7.4|8.*",
    "ext-json": "*",
    "nesbot/carbon": "2.48.*",
    "guzzlehttp/guzzle": "^7.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^2.17",
    "phpunit/phpunit": "^9.5",
    "spatie/ray": "^1.10",
    "vimeo/psalm": "^4.3",
    "phpstan/phpstan": "0.12.*",
    "squizlabs/php_codesniffer": "3.6.0"
  },
  "autoload": {
    "psr-4": {
      "Xnekv03\\ApiNameday\\": "src"
    },
    "files": [
      "src/helpers/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Xnekv03\\ApiNameday\\Tests\\": "tests"
    }
  },
  "scripts": {
    "psalm": "vendor/bin/psalm",
    "test": "vendor/bin/phpunit",
    "test-coverage": "vendor/bin/phpunit --coverage-html coverage",
    "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
